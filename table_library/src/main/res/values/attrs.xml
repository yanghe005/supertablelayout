<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="expand_height_list_view">
        <attr name="max_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="table_layout">
        <attr name="width" format="dimension" />
        <attr name="height" format="dimension" />
        <attr name="text_size" format="dimension" />
        <attr name="table_header_fixed" format="boolean" />
        <attr name="item_width" format="dimension" />
        <attr name="item_height" format="dimension" />
        <!-- 只有在item_width为0的情况下才生效 -->
        <attr name="item_left_right_margin" format="dimension" />
        <attr name="item_bg_pure_color" format="color" />
        <attr name="item_bg_interval_color1" format="color" />
        <attr name="item_bg_interval_color2" format="color" />
        <attr name="text_color" format="color" />
        <!-- 默认end -->
        <attr name="item_text_ellipsize" format="string" />
        <!-- 需要固定表头 -->
        <attr name="table_header_bg_color" format="color" />
        <!-- 需要固定行头 -->
        <attr name="row_header_bg_color" format="color" />
        <!-- 行合并设置(第一列(行头)和其他列不支持合并) -->
        <!-- 每组设置使用;进行分割，每组之中使用|进行行数和列数分割，列数中使用,进行分割(第一行和第一列均从0作为起点)  例如第2行合并3-5单元格和第4行合并1-2单元 1|2,4;3|0,1 -->
        <attr name="row_item_merge_index" format="string" />
    </declare-styleable>
</resources>